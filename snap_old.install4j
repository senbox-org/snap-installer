              <formComponent id="232" beanClass="com.install4j.runtime.beans.formcomponents.FileChooserComponent">
                <serializedBean>
                  <property name="initialBrowserDirectory" type="string">${installer:sys.userHome}</property>
                  <property name="labelText" type="string">Python executable:</property>
                  <property name="validationScript">
                    <object class="com.install4j.api.beans.ScriptProperty">
                      <property name="value" type="string">boolean success = true;

String pythonExecutable = file.getPath();
if (!pythonExecutable.isEmpty()) {
    ProcessBuilder pb = new ProcessBuilder().command(new String[]{file.getPath(), "--version"});
    try {
        Process p = pb.start();
        // TODO: redirect Python output (= Python version) and validate
        int exitCode = p.waitFor();
        if (exitCode != 0) {
            Util.showErrorMessage("'" + file.getName() + "' failed with exit code " + exitCode);
            success = false;
        }
    } catch (Throwable t) {
        Util.showErrorMessage("'" + file.getName() + "' failed: " + t.getMessage());
        success = false;
    }
}

return success;
</property>
                    </object>
                  </property>
                  <property name="variableName" type="string">pythonExecutable</property>
                </serializedBean>
                <initScript>if (Util.isWindowsInstaller()) {
    String[] paths = new String[]{"C:\\Python34", "C:\\Python33", "C:\\Python27"};
    for (String path : paths) {
        File file = new File(path, "python.exe");
        if (file.exists()) {
            configurationObject.setText(file.getPath());
            break;
        }
    }
} else {
    String[] commands = new String[]{"python3", "python"};
    for (String command : commands) {
        try {
            Runtime.getRuntime().exec(command);
            configurationObject.setText(command);
            break;
        } catch (Exception e) {
            // ok
        }
    }
}
</initScript>
              </formComponent>
